<%- include ("footer") %>
    <%- include ("header") %>
        <div class="container">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th class="col-6">Usuario</th>
                        <th class="col-6">Product</th>
                    </tr>
                </thead>
                <tbody>
                    <% comandes.forEach((comanda, index)=>{ %>
                        <tr>
                            <td class="col-6">
                                <%= comanda.UsuarioNombre %>
                            </td>   
                            <td class="col-6">
                                <%= comanda.email %>
                            </td>   
                            <td class="col-6">
                                <%= comanda.ProductName %>
                            </td>   
                            <td class="col-6">
                                <%= comanda.precio %>
                            </td>   
                        </tr>
                        <% }) %>
                </tbody>
            </table>
        </div>
        <%- include ("footer") %>